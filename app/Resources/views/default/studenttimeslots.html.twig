{% extends 'base.html.twig' %}

{% block body %}
    <div class="container">

        <div class="starter-template">
            <h1>Tests that I have signed up for</h1>
            <ul>
                {% for result in results %}
                    <li>{{ result.testName }} Date and Time: {{ result.startTime|date('m/d/Y H:i') }} to {{ result.endTime|date('m/d/Y H:i') }}</li>
                {% endfor %}
            </ul>
        </div>

    </div>
{% endblock %}


